class Solution {
    public int subtractProductAndSum(int n) {
        String s=n+"";
        int sum=1;
        int pr=0;
        for(int i=0;i<s.length();i++){
          sum*=Integer.parseInt(s.charAt(i)+"");
          pr+=Integer.parseInt(s.charAt(i)+"");
        }
        return sum-pr;
    }
}
