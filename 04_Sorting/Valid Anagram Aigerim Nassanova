class Solution {
    public boolean isAnagram(String s, String t) {
        char[] ss = s.toCharArray();
        char[] tt = t.toCharArray();
        Arrays.sort(ss);
        Arrays.sort(tt);
        String ans1 = new String(ss);
        String ans2 = new String(tt);
        if (ans1.equals(ans2)){
            return true;
        }
        return false;
    }
}
